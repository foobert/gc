{
  "name": "gc-rest",
  "version": "0.0.1",
  "description": "manages the geocache database abstraction",
  "repository": {
    "type": "git",
    "url": "http://github.com/foobert/gc.git"
  },
  "scripts": {
    "start": "node_modules/.bin/coffee index.coffee",
    "test": "node_modules/.bin/mocha",
    "dev": "docker run --rm -it -v $PWD:/usr/src/app -w /usr/src/app --link db:db -p 8081:8081 iojs node_modules/.bin/nodemon --exec node_modules/.bin/coffee index.coffee"
  },
  "dependencies": {
    "bluebird": "^2.9.13",
    "body-parser": "^1.12.2",
    "coffee-script": "^1.9.1",
    "csv": "^0.4.2",
    "debug": "^2.2.0",
    "express": "^4.12.2",
    "pg": "^4.3.0",
    "squel": "^3.10.0",
    "superagent-as-promised": "^2.0.0",
    "uuid": "^2.0.1",
    "xml2js": "^0.4.9"
  },
  "devDependencies": {
    "chai": "^2.1.2",
    "nodemon": "^1.3.7"
  }
}
